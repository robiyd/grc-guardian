{
  "env": "prod",
  "region": "us-west-2",
  "scope": "ALL",
  "steps": [
    {
      "description": "Check S3 bucket compliance",
      "params": {
        "rules": [
          "s3-bucket-public-read-prohibited",
          "s3-bucket-public-write-prohibited",
          "s3-bucket-server-side-encryption-enabled",
          "s3-bucket-versioning-enabled"
        ]
      },
      "step_number": 1,
      "tool": "aws_config_eval"
    },
    {
      "description": "Check IAM compliance",
      "params": {
        "rules": [
          "root-account-mfa-enabled",
          "iam-user-mfa-enabled",
          "access-keys-rotated",
          "iam-password-policy"
        ]
      },
      "step_number": 2,
      "tool": "aws_config_eval"
    },
    {
      "description": "Check CloudTrail compliance",
      "params": {
        "rules": [
          "cloudtrail-enabled"
        ]
      },
      "step_number": 3,
      "tool": "aws_config_eval"
    }
  ]
}
